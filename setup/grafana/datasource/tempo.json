{
  "uid": "tempo-v0-2",
  "name": "tempo v0.2",
  "type": "tempo",
  "url": "http://tempo:3200",
  "access":"proxy",
  "jsonData": {
    "nodeGraph": {
      "enabled": true
    },
    "serviceMap": {
      "datasourceUid": "prometheus-v0-2"
    },
    "tracesToLogsV2": {
      "customQuery": true,
      "datasourceUid": "loki-v0-2",
      "query": "{${__tags}} | json | traceid=\"${__span.traceId}\" | spanid=\"${__span.spanId}\"",
      "spanEndTimeShift": "2m",
      "spanStartTimeShift": "-2m"
    },
    "tracesToMetrics": {
      "datasourceUid": "prometheus-v0-2",
      "queries": [
        {
          "name": "Related metrics",
          "query": "{$__tags}"
        }
      ],
      "tags": [
        {
          "key": "service.name",
          "value": "service_name"
        }
      ]
    },
    "tracesToProfiles": {
      "datasourceUid": "pyroscope-v0-2",
      "profileTypeId": "process_cpu:cpu:nanoseconds:cpu:nanoseconds",
      "tags": [
        {
          "key": "service.name",
          "value": "service_name"
        }
      ]
    }
  }
}
